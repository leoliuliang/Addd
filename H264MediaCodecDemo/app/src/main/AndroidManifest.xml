<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mxkj.h264mediacodecdemo">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".camera.Camera2Activity"></activity>
        <activity android:name=".audio.MixAudioVideoActivity" />
        <activity android:name=".audio.ClipAudioActivity" />
        <activity android:name=".videochat.B.BChatActivity"></activity>
        <activity android:name=".videochat.A.AChatActivity" />
        <activity android:name=".touping.play.ToupingPlayerActivity" />
        <activity android:name=".touping.PushActivity" />
        <activity android:name=".mediacodec.H264EncoderActivity" />
        <activity android:name=".player.DecodeH264Activity" />
        <activity android:name=".camera.CameraActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>